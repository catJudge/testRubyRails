<div class="container">
  <div class="row">
    <div class="col-md-5">
      <h3>Users</h3>
      <table class="table table-hover">
        <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Admin</th>
        </tr>
        </thead>
        <tbody>
        <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

